@model IEnumerable<frontEndFyp.Models.Decoration>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Decoration_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Restaurant.Restaurant_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Decoration_Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Decoration_Price)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Decoration_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Restaurant.Restaurant_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Decoration_Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Decoration_Price)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Decoration_Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Decoration_Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Decoration_Id })
            </td>
        </tr>
    }

</table>
