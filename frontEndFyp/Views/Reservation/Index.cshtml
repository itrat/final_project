                <td>
                    @Html.DisplayFor(modelItem => item.Food.Food_Item)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Restaurant.Restaurant_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.User.User_F_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.User.User_L_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Event.Event_Type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Reservation_Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Reservation_Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Reservation_Id })
                </td>
            </tr>
        }

    </table>
</body>
</html>
